<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:ex="http://example.com/ontology#"
         xml:base="http://example.com/ontology">

  <!-- Ontology Declaration -->
  <owl:Ontology rdf:about="http://example.com/ontology">
    <rdfs:label>SWRL Model Selection Ontology</rdfs:label>
    <rdfs:comment>RDF 온톨로지 for 규칙 기반 모델 선택 시스템</rdfs:comment>
  </owl:Ontology>

  <!-- Classes -->

  <!-- QueryGoal Class -->
  <owl:Class rdf:about="http://example.com/ontology#QueryGoal">
    <rdfs:label>Query Goal</rdfs:label>
    <rdfs:comment>사용자의 목표 요청을 나타내는 클래스</rdfs:comment>
  </owl:Class>

  <!-- Model Class -->
  <owl:Class rdf:about="http://example.com/ontology#Model">
    <rdfs:label>Model</rdfs:label>
    <rdfs:comment>실행 가능한 AI 모델을 나타내는 클래스</rdfs:comment>
  </owl:Class>

  <!-- Parameter Class -->
  <owl:Class rdf:about="http://example.com/ontology#Parameter">
    <rdfs:label>Parameter</rdfs:label>
    <rdfs:comment>QueryGoal의 매개변수를 나타내는 클래스</rdfs:comment>
  </owl:Class>

  <!-- OutputSpec Class -->
  <owl:Class rdf:about="http://example.com/ontology#OutputSpec">
    <rdfs:label>Output Specification</rdfs:label>
    <rdfs:comment>예상 출력 형식을 나타내는 클래스</rdfs:comment>
  </owl:Class>

  <!-- Object Properties -->

  <!-- selectedModel Property -->
  <owl:ObjectProperty rdf:about="http://example.com/ontology#selectedModel">
    <rdfs:label>selected model</rdfs:label>
    <rdfs:comment>QueryGoal과 선택된 Model 간의 관계</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#QueryGoal"/>
    <rdfs:range rdf:resource="http://example.com/ontology#Model"/>
  </owl:ObjectProperty>

  <!-- hasParameter Property -->
  <owl:ObjectProperty rdf:about="http://example.com/ontology#hasParameter">
    <rdfs:label>has parameter</rdfs:label>
    <rdfs:comment>QueryGoal이 Parameter를 가지는 관계</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#QueryGoal"/>
    <rdfs:range rdf:resource="http://example.com/ontology#Parameter"/>
  </owl:ObjectProperty>

  <!-- hasOutputSpec Property -->
  <owl:ObjectProperty rdf:about="http://example.com/ontology#hasOutputSpec">
    <rdfs:label>has output spec</rdfs:label>
    <rdfs:comment>QueryGoal이 OutputSpec을 가지는 관계</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#QueryGoal"/>
    <rdfs:range rdf:resource="http://example.com/ontology#OutputSpec"/>
  </owl:ObjectProperty>

  <!-- Data Properties -->

  <!-- QueryGoal Properties -->
  <owl:DatatypeProperty rdf:about="http://example.com/ontology#goalId">
    <rdfs:label>goal ID</rdfs:label>
    <rdfs:comment>QueryGoal의 고유 식별자</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#QueryGoal"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/ontology#goalType">
    <rdfs:label>goal type</rdfs:label>
    <rdfs:comment>QueryGoal의 유형 (예: predict_job_completion_time)</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#QueryGoal"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Model Properties -->
  <owl:DatatypeProperty rdf:about="http://example.com/ontology#modelId">
    <rdfs:label>model ID</rdfs:label>
    <rdfs:comment>Model의 고유 식별자</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#Model"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/ontology#purpose">
    <rdfs:label>purpose</rdfs:label>
    <rdfs:comment>Model의 목적 (예: DeliveryPrediction)</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#Model"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/ontology#version">
    <rdfs:label>version</rdfs:label>
    <rdfs:comment>Model의 버전</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#Model"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/ontology#metaDataFile">
    <rdfs:label>metadata file</rdfs:label>
    <rdfs:comment>Model의 메타데이터 파일 경로</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#Model"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Parameter Properties -->
  <owl:DatatypeProperty rdf:about="http://example.com/ontology#parameterKey">
    <rdfs:label>parameter key</rdfs:label>
    <rdfs:comment>Parameter의 키</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#Parameter"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/ontology#parameterValue">
    <rdfs:label>parameter value</rdfs:label>
    <rdfs:comment>Parameter의 값</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#Parameter"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- OutputSpec Properties -->
  <owl:DatatypeProperty rdf:about="http://example.com/ontology#outputName">
    <rdfs:label>output name</rdfs:label>
    <rdfs:comment>OutputSpec의 이름</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#OutputSpec"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.com/ontology#outputDatatype">
    <rdfs:label>output datatype</rdfs:label>
    <rdfs:comment>OutputSpec의 데이터 타입</rdfs:comment>
    <rdfs:domain rdf:resource="http://example.com/ontology#OutputSpec"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

</rdf:RDF>